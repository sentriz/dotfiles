_main() {
    # matches scripts below with "--aguments"
    _script_commands="$(egrep -oh "\s[\"\']\-{2}\w*[\"\']\s" $(which $1) | tr -d '\n' | tr -s ' ' | tr -d \")"
    # matches scripts below with "    arguments)" (case/esac)
    _script_commands="$_script_commands $(egrep -oh "\s{4}\w*\)" $(which $1) | tr -d '\n' | tr -s ' ' | tr -d ")")"
    local cur
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    COMPREPLY=($(compgen -W "${_script_commands}" -- ${cur}))
    return 0
}

complete -o nospace -F _main scratch
complete -o nospace -F _main projector
complete -o nospace -F _main now_playing
complete -o nospace -F _main screenshot
