[calendars]

[[calendar]]
path = ~/calendar/calendar/
type = calendar

[[birthdays]]
path = ~/calendar/birthdays/
type = calendar

[[work]]
path = ~/calendar/work/
type = calendar

[locale]
timeformat = %H:%M
dateformat = %Y/%m/%d
longdateformat = %Y/%m/%d
datetimeformat = %Y/%m/%d %H:%M
longdatetimeformat = %Y/%m/%d %H:%M

[default]
default_calendar = calendar
