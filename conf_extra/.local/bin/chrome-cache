#!/usr/bin/env bash

rm -r "$HOME/.var/app/com.github.Eloston.UngoogledChromium/cache"
rm -r "$HOME/.var/app/com.github.Eloston.UngoogledChromium/config/chromium/Default/Service Worker"
rm -r "$HOME/.var/app/com.github.Eloston.UngoogledChromium/config/chromium/Default/IndexedDB"
rm -r "$HOME/.var/app/com.github.Eloston.UngoogledChromium/config/chromium/Default/Favicons"
rm -r "$HOME/.var/app/com.github.Eloston.UngoogledChromium/config/chromium/Floc"
rm -r "$HOME/.var/app/com.github.Eloston.UngoogledChromium/config/chromium/GrShaderCache"
rm -r "$HOME/.var/app/com.github.Eloston.UngoogledChromium/config/chromium/AutofillStates"
rm -r "$HOME/.var/app/com.github.Eloston.UngoogledChromium/config/chromium/Fav"

rm -r "$HOME/.var/app/com.google.Chrome/cache"
rm -r "$HOME/.var/app/com.google.Chrome/config/google-chrome/Default/Service Worker"
rm -r "$HOME/.var/app/com.google.Chrome/config/google-chrome/Default/IndexedDB"
rm -r "$HOME/.var/app/com.google.Chrome/config/google-chrome/Default/Favicons"
rm -r "$HOME/.var/app/com.google.Chrome/config/google-chrome/Floc"
rm -r "$HOME/.var/app/com.google.Chrome/config/google-chrome/GrShaderCache"
rm -r "$HOME/.var/app/com.google.Chrome/config/google-chrome/AutofillStates"
